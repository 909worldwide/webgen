{
    "manifest_version": 2,
    "name": "WebGen",
    "version": "1.0",
    "description": "ChatGPT literally integrated into Web pages. Answers in-line",
    "icons": {
      "48": "icons/border-48.png"
    },
    "permissions": [
      "contextMenus",
      "storage",
      "activeTab",
      "*://*/*"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    }
  }
  